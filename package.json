{
  "name": "trainer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "tsc && next build --debug",
    "start": "next start -p $PORT",
    "local-start": "next start",
    "test": "jest --watch",
    "testCoverage": "jest --watchAll --coverage",
    "test-ci": "jest --coverage --ci",
    "analysis": "ANALYZE=true yarn build",
    "clean": "npx rimraf build out",
    "export": "next build && next export",
    "docker-build": "docker buildx build --build-arg BUILD_ID=$(git rev-parse --verify HEAD) -t incheon-web ./",
    "docker-build-m1": "docker buildx build --platform linux/amd64 --build-arg BUILD_ID=$(git rev-parse --verify HEAD) -t incheon-web ./"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.14.1",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "next": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.6",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "intercept-stdout": "^0.1.2",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-styled-components": "^7.1.1",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
